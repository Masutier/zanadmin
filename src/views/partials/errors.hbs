<!-- From Passport -->
{{#if error_msg }}
    <div class="alert alert-danger alert-dismissible fade show" role="alert">
        {{ error_msg }}
        <button type="button" class="close" data-dismiss="alert" aria-label="Close"></button>
    </div>
{{/if }}


{{#each errors_msg }}
    <div class="alert alert-danger alert-dismissible fade show" role="alert">
        {{ text }}
        <button type="button" class="close" data-dismiss="alert" aria-label="Close"></button>
    </div>
{{/each }}
